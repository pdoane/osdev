// Write solid color (0xFF8040FF) pixel Shader

/////////////////////////////////////////////////////////////////////////////////////////////
// Kernel 0: 8-pixel Dispatch - SIMD8 mode
//
// Input
//
//  7    6    5    4    3    2    1    0       GRF
//               PS Header                   - r0
//               PS Header                   - r1
// (Note: No payload as no attributes)
//
//
// Output
//  7    6    5    4    3    2    1    0
// C7.r C6.r C5.r C4.r C3.r C2.r C1.r C0.r   - r124
// C7.g C6.g C5.g C4.g C3.g C2.g C1.g C0.g   - r125
// C7.b C6.b C5.b C4.b C3.b C2.b C1.b C0.b   - r126
// C7.a C6.a C5.a C4.a C3.a C2.a C1.a C0.a   - r127
//

mov (1) r2<1>:f 0x30201030:vf  // Unpack immediate vector
mov (8) r124<1>:f r2.0<0;1,0>:f   // Copy red to output
mov (8) r125<1>:f r2.1<0;1,0>:f   // Copy green to output
mov (8) r126<1>:f r2.2<0;1,0>:f   // Copy blue to output
mov (8) r127<1>:f r2.3<0;1,0>:f   // Copy alpha to output
send (8) null r124:f 0x25 0x08019400        // Send EOT | Render Cache DP, RENDER_TARGET_WRITE | LAST_RENDER_TARGET | SLOTGRP_LO | SIMD8_LO | BINDING_TABLE_IDX(0)



